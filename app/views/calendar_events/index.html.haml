#main.container
    %h3.header Calendar Events For HCS-PTO

    %table.table
        %thead
            %tr
                %th Title
                %th Description
                %th Location
                %th Contact Person
                %th Start_Date
                %th End_Date
        %tbody
            -@calendar_event.each do |event|
                %tr
                %td=event.title
                %td=event.description
                %td=event.location
                %td=event.contact_person
                %td=event.start_date_time
                %td=event.end_date_time
=link_to "Create a new event for HCS-PTO", new_calendar_event_path
